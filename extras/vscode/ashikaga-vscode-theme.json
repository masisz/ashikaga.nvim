  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A6A8AB"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#E5E7EB"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#E5E7EB"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#C44f4f"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#BFA8D5"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
